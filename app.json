{
  "expo": {
    "name": "mobdan",
    "slug": "mobdan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "mobdan",
    "userInterfaceStyle": "automatic",
    "backgroundColor":"#16171D",
    "splash": {
      "image":"./assets/images/logo.png",
      "resizeMode":"contain",
      "backgroundColor":"#16171D"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#16171D"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
